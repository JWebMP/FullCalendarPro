# Environment keys follow rules/generative/platform/secrets-config/env-variables.md

# Organization/project metadata
ORG_NAME=JWebMP
ORG_SUPPORT_EMAIL=contact@jwebmp.com
PROJECT_NAME=fullcalendar-pro

# Runtime configuration
ENVIRONMENT=dev
PORT=8080
BASE_URL=http://localhost:8080/fullcalendar-pro
SERVICE_NAME=fullcalendar-pro
LOGGING_BACKEND=cloud-logging
TRACING_ENABLED=true
GCP_TRACE_EXPORT=true

# Auth / security (replace placeholders before release)
OAUTH2_ISSUER_URL=https://auth.example.com
OAUTH2_REALM=fullcalendar
OAUTH2_CLIENT_ID=fullcalendar-pro-client
OAUTH2_CLIENT_SECRET=change-me
JWKS_URI=https://auth.example.com/realms/fullcalendar/protocol/openid-connect/certs
INTROSPECTION_URL=https://auth.example.com/realms/fullcalendar/protocol/openid-connect/token/introspect
AUTH_ADMIN_USER=admin
AUTH_ADMIN_PASS=change-me

# Database / persistence
DB_URL=jdbc:postgresql://localhost:5432/fullcalendar
DB_USER=fullcalendar
DB_PASS=change-me
DB_POOL_SIZE=20
PG_PASSWORD_SECRET=prod-key-fullcalendar

# Testing overrides
TEST_DB_CONTAINER_IMAGE=postgres:17
ENABLE_DEBUG_LOGS=true
SKIP_INTEGRATION_TESTS=false

# Secrets and service tokens
KEYCLOAK_ADMIN_PASSWORD=change-me
POSTGRES_KEYCLOAK_PASSWORD=change-me
POSTGRES_APP_PASSWORD=change-me
JWT_TEST_TOKEN=eyJhbGciOi...
